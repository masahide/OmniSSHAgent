{
  "name": "ssh-agent-win",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "start": "sirv dist --no-clear",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile dist/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile dist/smui-dark.css -i src/theme/dark"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-image": "^2.1.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@smui/button": "^6.0.0-beta.14",
    "@smui/card": "^6.0.0-beta.14",
    "@zerodevx/svelte-toast": "^0.7.0",
    "autoprefixer": "^10.4.2",
    "postcss": "^8.4.6",
    "postcss-url": "^10.1.3",
    "rollup": "^2.67.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-watch": "^4.3.1",
    "smui-theme": "^6.0.0-beta.14",
    "svelte": "^3.46.4",
    "svelte-atoms": "^0.0.27",
    "svelte-file-dropzone": "^1.0.0-dev.1"
  },
  "dependencies": {
    "sirv-cli": "^2.0.2",
    "svelte-material-ui": "^6.0.0-beta.14"
  },
  "author": "YAMASAKI Masahide"
}
